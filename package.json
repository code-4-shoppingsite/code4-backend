    "express-session": "^1.17.0",
    "fs": "0.0.1-security",
    "handlebars": "^4.7.6",
    "hogan.js": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.5",
